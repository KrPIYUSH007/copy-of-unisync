{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/api/index.js"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "/backend/uploads/$1"
    }
  ],
  "functions": {
    "backend/api/index.js": {
      "maxDuration": 10
    }
  }
}